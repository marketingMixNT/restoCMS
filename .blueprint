created:
    - database/factories/CategoryFactory.php
    - database/factories/MenuFactory.php
    - database/factories/DishFactory.php
    - database/migrations/2024_07_01_071605_create_categories_table.php
    - database/migrations/2024_07_01_071606_create_menus_table.php
    - database/migrations/2024_07_01_071607_create_dishes_table.php
    - app/Models/Category.php
    - app/Models/Menu.php
    - app/Models/Dish.php
models:
    User: { name: string, email: string, email_verified_at: 'timestamp nullable default:NULL', password: string, remember_token: 'string:100 nullable default:NULL' }
    Category: { name: string, sort: 'integer nullable', relationships: { hasMany: Menu } }
    Menu: { category_id: id, sort: 'integer nullable', relationships: { belongsTo: Category, hasMany: Dish } }
    Dish: { name: text, ingredients: text, price: integer, menu_id: id, sort: 'integer nullable', relationships: { belongsTo: Menu } }
